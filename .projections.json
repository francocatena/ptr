{
  "lib/ptr_web/channels/*_channel.ex": {
    "type": "channel",
    "dispatch": "mix test test/ptr_web/channels/{}_channel_test.exs",
    "alternate": "test/ptr_web/channels/{}_channel_test.exs"
  },

  "lib/ptr_web/controllers/*_controller.ex": {
    "type": "controller",
    "dispatch": "mix test test/ptr_web/controllers/{}_controller_test.exs",
    "alternate": "test/ptr_web/controllers/{}_controller_test.exs"
  },

  "lib/ptr_web/views/*_view.ex": {
    "type": "view",
    "dispatch": "mix test test/ptr_web/views/{}_view_test.exs",
    "alternate": "test/ptr_web/views/{}_view_test.exs"
  },

  "lib/ptr_web/views/*_helpers.ex": {
    "type": "helper",
    "dispatch": "mix test test/ptr_web/views/{}_helpers_test.exs",
    "alternate": "test/ptr_web/views/{}_helpers_test.exs"
  },

  "lib/ptr_web/plugs/*_plug.ex": {
    "type": "plug",
    "dispatch": "mix test test/ptr_web/plugs/{}_plug_test.exs",
    "alternate": "test/ptr_web/plugs/{}_plug_test.exs"
  },

  "priv/gettext/*.po": {
    "type": "locale"
  },

  "test/ptr_web/channels/*_channel_test.exs": {
    "type": "tchannel",
    "dispatch": "mix test {file}",
    "alternate": "lib/ptr_web/channels/{}_channel.ex"
  },

  "test/ptr_web/controllers/*_controller_test.exs": {
    "type": "tcontroller",
    "dispatch": "mix test {file}",
    "alternate": "lib/ptr_web/controllers/{}_controller.ex"
  },

  "test/ptr_web/views/*_view_test.exs": {
    "type": "tview",
    "dispatch": "mix test {file}",
    "alternate": "lib/ptr_web/views/{}_view.ex"
  },

  "test/ptr_web/views/*_helpers_test.exs": {
    "type": "thelper",
    "dispatch": "mix test {file}",
    "alternate": "lib/ptr_web/views/{}_helpers.ex"
  },

  "test/ptr_web/plugs/*_plug_test.exs": {
    "type": "tplug",
    "dispatch": "mix test {file}",
    "alternate": "lib/ptr_web/plugs/{}_plug.ex"
  },

  "test/ptr/*_test.exs": {
    "type": "tapp",
    "dispatch": "mix test {file}",
    "alternate": "lib/ptr/{}.ex"
  },

  "test/support/*.ex": {
    "type": "tsupport"
  },

  "lib/ptr_web/templates/*.eex": {
    "type": "template"
  },

  "lib/ptr_web/router.ex": {
    "type": "router"
  },

  "assets/css/app.scss": {
    "type": "stylesheet"
  },

  "assets/css/*": {
    "type": "stylesheet"
  },

  "assets/js/app.js": {
    "type": "javascript",
    "dispatch": "npm -s --prefix assets run lint {file}"
  },

  "assets/js/*": {
    "type": "javascript",
    "dispatch": "npm -s --prefix assets run lint {file}"
  },

  "config/*.exs": {
    "type": "environment"
  },

  "lib/ptr.ex": {
    "type": "app"
  },

  "lib/ptr/*.ex": {
    "type": "app",
    "dispatch": "mix test test/ptr/{}_test.exs",
    "alternate": "test/ptr/{}_test.exs"
  },

  "lib/ptr_web.ex": {
    "type": "web"
  },

  "lib/ptr_web/*": {
    "type": "web"
  },

  "priv/repo/seeds.exs": {
    "type": "seed"
  },

  "priv/repo/migrations/*.exs": {
    "type": "migration"
  },

  "mix.exs": {
    "type": "mix"
  }
}
